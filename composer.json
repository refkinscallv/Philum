{
    "name": "refkinscallv/philum",
    "description": "Simple, Lightweight and Modular traditional PHP Framework. With MVC, Routing and Query Builder/ORM",
    "type": "project",
    "version": "1.0.7",
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "illuminate/database": "^10.48",
        "phpmailer/phpmailer": "^6.9",
        "refkinscallv/router": "^1.0",
        "refkinscallv/http": "^1.0",
        "refkinscallv/crypto": "^1.0",
        "refkinscallv/cookie": "^1.0",
        "refkinscallv/session": "^1.0"
    },
    "require-dev": {
        "vlucas/phpdotenv": "^5.6",
        "spatie/ignition": "^1.15",
        "illuminate/database": "^10.48",
        "phpmailer/phpmailer": "^6.9",
        "refkinscallv/router": "^1.0",
        "refkinscallv/http": "^1.0",
        "refkinscallv/crypto": "^1.0",
        "refkinscallv/cookie": "^1.0",
        "refkinscallv/session": "^1.0",
        "phpunit/phpunit": "^10.5"
    },
